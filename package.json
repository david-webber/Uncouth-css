{
  "name": "uncouth-css",
  "version": "1.0.0",
  "description": "Another classless CSS library",
  "main": "src/main.scss",
  "devDependencies": {
    "css-loader": "^5.0.1",
    "mini-css-extract-plugin": "^1.3.2",
    "node-sass": "^5.0.0",
    "sass": "^1.26.11",
    "sass-loader": "^10.1.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "scripts": {
    "light": "webpack --env theme='light' && rm dist/main.js",
    "dark": "webpack --env theme='dark' && rm dist/main.js",
    "auto": "webpack --env theme='auto' && rm dist/main.js",
    "build": "npm run light && npm run dark && npm run auto"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/david-webber/Uncouth-css.git"
  },
  "keywords": [
    "classless",
    "css",
    "library",
    "uncouth",
    "uncouth-css"
  ],
  "author": "David Webber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/david-webber/Uncouth-css/issues"
  },
  "homepage": "https://github.com/david-webber/Uncouth-css#readme"
}
